[application]

config/icon = "res://icon.png"
config/name = "TileSetWizard"
run/disable_stdout = true
run/main_scene = "res://scenes/Main.tscn"

[display]

window/size/height = 600
window/size/width = 1024
window/stretch/aspect = "keep"
window/stretch/mode = "2d"

[importer_defaults]

texture = {
"flags/filter": true,
"flags/mipmaps": true
}

[physics_2d]

motion_fix_enabled = true

